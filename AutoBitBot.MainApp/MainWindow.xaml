<Window x:Class="AutoBitBot.MainApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:uc="clr-namespace:AutoBitBot.MainApp.UserControls"
        xmlns:models="clr-namespace:AutoBitBot.MainApp.Infrastructure.ViewModels;assembly=AutoBitBot.MainApp.Infrastructure"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="800">
    <!--<Window.DataContext>
        <models:BitTaskSchedulerViewModel />
    </Window.DataContext>-->

    <StackPanel>
        <Button CommandParameter="{Binding}" Command="{Binding Path=ImmediatelySellAfterBuyCommand}">SellBuy Command</Button>
        <uc:BitTaskSchedulerControl />
        <uc:MarketTickerControl />
        <uc:BalanceControl />
        <uc:OutputControl x:Name="OutputControl" />
    </StackPanel>
</Window>
